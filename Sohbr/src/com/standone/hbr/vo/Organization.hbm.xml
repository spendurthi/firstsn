<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.standone.hbr.vo.Organization" table="ORGANIZATION">
        <id name="orgId" type="int" column="ORG_ID">
            <generator class="increment" />
        </id>
        <property name="orgName" type="string" not-null="true" length="25" column="ORG_NAME" />
    </class>
</hibernate-mapping>
