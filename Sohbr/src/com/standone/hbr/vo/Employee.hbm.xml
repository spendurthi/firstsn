<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.standone.hbr.vo.Employee" table="EMPLOYEE">
        <id name="empId" type="int" column="EMP_ID">
            <generator class="increment" />
        </id>
        <property name="empName" type="string" not-null="true" length="25" column="EMP_NAME" />
        <many-to-one name="deptId" column="DEPT_ID" class="com.standone.hbr.vo.Department" not-null="true" />
    </class>
</hibernate-mapping>
